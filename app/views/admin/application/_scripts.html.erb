<% Administrate::Engine.javascripts.each do |js_path| %>
  <%= javascript_include_tag js_path, 'data-turbolinks-suppress-warning':'true' %>
<% end %>

<%= yield :scripts %>

<% if Rails.env.test? %>
  <%= javascript_tag do %>
    $.fx.off = true;
    $.ajaxSetup({ async: false });
  <% end %>
<% end %>
